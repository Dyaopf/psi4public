import os
import shutil
import subprocess

build  = '@CMAKE_BINARY_DIR@'
scripts = build+'/doc/cloc/matplotlib'

for root, dirs, filenames in os.walk(scripts):
    for f in filenames:
        execfile(scripts + '/' + f)

