
SHELL = /bin/sh

#
# Initialize variables that have been figured out by configure.
#
target = i386-apple-darwin11.0.0
prefix = /usr/local/psi4
docdir = ${datarootdir}/doc/${PACKAGE_TARNAME}
datarootdir = ${prefix}/share
datadir = ${datarootdir}
dvidir = $(docdir)/dvi
psdir = $(docdir)/ps
pdfdir = $(docdir)/pdf
htmldir = $(docdir)/html
top_srcdir = /Users/masaaki/psi4
# this file is used by the subdirectories, so top_objdir is relative to those
top_objdir = ../..
currdir = $(shell echo `pwd`)

LATEX = 
BIBTEX = 
DVIPS = 
PS2PDF = ps2pdf
LATEX2HTML = 
DOXYGEN = NO
HAVE_DOT = NO
DOT_PATH = 
MKDIRS = $(top_srcdir)/bin/mkdirs.sh
INSTALL = /usr/bin/install -c
INSTALL_DATA = ${INSTALL} -m 644

TEXINPUTS = :$(srcdir):$(srcdir)/..:..:

TARGET_DVI  = $(MAN).dvi
TARGET_PS = $(MAN).ps
TARGET_PDF = $(MAN).pdf
TARGET_HTML = $(MAN).html

ifndef LATEX2HTML
  TARGET_HTML =
endif

ifndef PS2PDF
  TARGET_PDF =
endif

ifndef DVIPS
  TARGET_PS =
  TARGET_PDF =
  TARGET_HTML =
endif

ifndef LATEX
  TARGET_DVI =
  TARGET_PS =
  TARGET_PDF =
  TARGET_HTML =
endif

vpath %.html $(currdir)/html:
vpath %.tex $(srcdir):$(srcdir)/..
vpath %.bib $(srcdir):
