#! UHF 6-31G** energy of the $^3B_1$ state of CH$_2$, with Z-matrix input.

nucenergy = 6.648419426607498
refenergy = -38.91591816165388

molecule ch2 {
    0 3
    c
    h 1 b1
    h 1 b1 2 a1

    b1 = 1.0
    a1 = 125.0
}

globals {
    reference uhf
    scf_type direct
    basis 6-31G**
    docc [2, 0, 0, 1]
    socc [1, 0, 1, 0]
}

thisenergy = energy('uhf')

compare_values(nucenergy, ch2.nuclear_repulsion_energy(), 9, "Nuclear repulsion energy")
compare_values(refenergy, thisenergy, 9, "Reference energy")

