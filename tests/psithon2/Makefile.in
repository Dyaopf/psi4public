
SRCDIR = @srcdir@

include ../MakeVars
PSIAUTOTEST = false

export PSIPATH:= $(SRCDIR)/psiaux2:$(SRCDIR)/psiaux1

$(TARGET):
	@SRCDIR=$(SRCDIR) PSIDATADIR=$(SRC_PSIDATADIR) $(PERL) ../runtest.pl $(SRCDIR)/input.dat $(TARGET) $(PSIAUTOTEST)

clean:
	@SRCDIR=$(SRCDIR) PSIDATADIR=$(SRC_PSIDATADIR) 
	/bin/rm -f $(TARGET) $(GARBAGE)

