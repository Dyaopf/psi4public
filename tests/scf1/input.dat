#%! RHF cc-pVQZ energy for the BH molecule, with Cartesian input.

nucenergy = 2.645886245000000
refenergy = -25.10354687361508

molecule bh {
    b      0.0000        0.0000        0.0000
    h      0.0000        0.0000        1.0000
}

globals {
    scf_type direct
    basis cc-pVQZ
    e_converge = 10
}

thisenergy = energy('rhf')

compare_values(nucenergy, bh.nuclear_repulsion_energy(), 9, "Nuclear repulsion energy")
compare_values(refenergy, thisenergy, 9, "Reference energy")

clean()
