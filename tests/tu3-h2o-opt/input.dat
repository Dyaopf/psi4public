#! Optimize H2O HF/cc-pVDZ

nucenergy = 9.300472711622458 #TEST
refenergy = -76.0270534721367 #TEST

molecule h2o {
  O 
  H 1 0.96
  H 1 0.96 2 104.5
}

set basis cc-pVDZ
optimize('scf')

compare_values(nucenergy, h2o.nuclear_repulsion_energy(), 4, "Nuclear repulsion energy")    #TEST
compare_values(refenergy, get_variable("CURRENT ENERGY"), 4, "Reference energy") #TEST
