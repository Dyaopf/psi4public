#! EOM-CC2/cc-pVDZ on H$@@2$O$@@2$ with two excited states in each irrep

memory 250 mb

molecule h2o2 {
  0 1
  O
  O 1 1.39
  H 1 0.94 2 102.3
  H 2 0.94 1 102.3 3 -50.0
}

set {
  basis cc-pVDZ
  freeze_core true
  states_per_irrep [2, 2]
  prop_all true
}

energy('eom-cc2')

PsiMod.set_global_option('WFN', 'EOM_CC2')
PsiMod.set_global_option('DERTYPE', 'RESPONSE')
PsiMod.set_global_option('JOBTYPE', 'OEPROP')
PsiMod.set_global_option('ONEPDM', 'TRUE')
PsiMod.cclambda()
PsiMod.ccdensity()

