#! EOM-CC2/cc-pVDZ on H$@@2$O$@@2$ with two excited states in each irrep

memory 250 mb

molecule h2o2 {
  0 1
  O
  O 1 1.39
  H 1 0.94 2 102.3
  H 2 0.94 1 102.3 3 -50.0
}

set {
  basis cc-pVDZ
  freeze_core true
  states_per_irrep [2, 2]
  prop_all true
  properties = ['oscillator' 'rotation' 'polarizability']
}

properties = ['test', 'polarizability', 'dipole', 'polarizability']

oneel_properties = [
  'dipole',
  'quadrupole',
]

twoel_properties = [
]

response_properties = [
  'polarizability',
  'optrot',
  'dipole-quadrupole'
]

one = []
two = []
response = []
invalid = []

for prop in properties:
  if prop in oneel_properties:
    one.append(prop)
  elif prop in twoel_properties:
    two.append(prop)
  elif prop in response_properties:
    response.append(prop)
  else:
    invalid.append(prop)

one = drop_duplicates(one)
two = drop_duplicates(two)
response = drop_duplicates(response)
invalid = drop_duplicates(invalid)

print "properties = %s\n" % properties
print "one = %s\n" % one
print "two = %s\n" % two
print "response = %s\n" % response
print "invalid = %s\n" % invalid

