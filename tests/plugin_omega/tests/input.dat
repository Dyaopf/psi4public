molecule {
0 1
O
H 1 1.0
H 1 1.0 2 104.5
symmetry c1
}

plugin_load("../plugin_omega.so")

set globals {
  basis aug-cc-pvdz 
  ri_basis_scf cc-pvdz-ri
  reference uhf
  scf_type df
}

energy('scf')

set globals {
  dft_functional wB97
  dft_order_spherical 25 
  dft_n_radial 35 
  omega_procedure ip
  maxiter 50
  d_converge 5
  e_converge 7
}

plugin("../plugin_omega.so")
