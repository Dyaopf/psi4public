
molecule h2o {
    O
    H 1 0.9584
    H 1 0.9584 2 104.45
    symmetry c1
}

plugin_load("./plugin_dfadc.so")

set {
    reference rhf
    wavefunction mp2
    basis aug-cc-pvdz
    ri_basis_cc  aug-cc-pvdz
    ri_basis_adc aug-cc-pvdz
    guess core
   
    num_roots 2
#    do_pr true
    convergence 11
    e_converge 11
    d_converge 11
}

scf()
run_dfcc(h2o)
plugin("plugin_dfadc.so")

clean()
