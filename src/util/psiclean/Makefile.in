
srcdir = @srcdir@
VPATH = @srcdir@

include ../MakeVars

CXXSRC = psiclean.cc
BINOBJ = $(CXXSRC:%.cc=%.o)
PSILIBS = -lPSI_chkpt -lPSI_psio -lPSI_ciomr -lPSI_ipv1
ALLOC =

include ../MakeRules

ifneq ($(DODEPEND),no)
$(BINOBJ:%.o=%.d): $(DEPENDINCLUDE)
include $(BINOBJ:%.o=%.d)
endif

install_man:: psiclean.1
	$(MKDIRS) $(DESTDIR)$(mandir)/man1
	$(INSTALL_PROGRAM) $(srcdir)/psiclean.1 $(DESTDIR)$(mandir)/man1
