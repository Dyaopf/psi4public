
SHELL = /bin/sh

#
# Initialize variables that have been figured out by configure.
#

target = @target@

prefix = @prefix@
exec_prefix = @exec_prefix@
bindir = @bindir@
libdir = @libdir@
incdir = @includedir@
docdir = @docdir@
manext = 5
mandir = @mandir@
htmldir = @htmldir@
txtdir = @txtdir@
datadir = @datadir@
datarootdir = @datarootdir@
top_srcdir = @top_srcdir@
# this file is used by the subdirectories, so top_objdir is relative to those
top_objdir = ../../..
objincdir = @objincdir@

PWD = $(shell pwd)
RANLIB = @RANLIB@
YACC = @YACC@
CC = @CC@
CXX = @CXX@
FC = @FC@
MKDIRS = @MKDIRS@
INSTALL = @INSTALL@
INSTALL_INCLUDE = @INSTALL_INCLUDE@
INSTALL_DATA = @INSTALL_DATA@
INSTALL_PROGRAM = @INSTALL_PROGRAM@
MKDIRS = @MKDIRS@

PSICPPFLAGS = @PSICPPFLAGS@
CPPFLAGS = @CPPFLAGS@

CDEF = @CDEF@
COPT = @COPT@
COTH = @COTH@
CDBG = @CDBG@
CINC = @CINC@

CXXDEF = @CXXDEF@
CXXOPT = @CXXOPT@
CXXOTH = @CXXOTH@
CXXDBG = @CXXDBG@
CXXINC = @CXXINC@

FDEF = @FDEF@
FOPT = @FOPT@
FOTH = @FOTH@
FDBG = @FDBG@
FINC = @FINC@
FSTATIC = @FSTATIC@

# The suffix generated by the -M compiler option
CCDEPENDSUF = @CCDEPENDSUF@
CXXDEPENDSUF = @CXXDEPENDSUF@
CCDEPENDFLAGS = @CCDEPENDFLAGS@
CXXDEPENDFLAGS = @CXXDEPENDFLAGS@
CCDEPEND = $(CC)
CXXDEPEND = $(CXX)

# Flags for Ruby location
RUBY_MAJOR             = @RUBY_CONFIG_MAJOR@
RUBY_MINOR             = @RUBY_CONFIG_MINOR@
RUBY_TEENY             = @RUBY_CONFIG_TEENY@
RUBY_VERSION_CODE      = @RUBY_CONFIG_MAJOR@@RUBY_CONFIG_MINOR@@RUBY_CONFIG_TEENY@
RUBY_CPPFLAGS          = @RUBY_CPPFLAGS@ -DRUBY_VERSION_CODE=$(RUBY_VERSION_CODE) \
                         -DRUBY_MAJOR=$(RUBY_MAJOR) -DRUBY_MINOR=$(RUBY_MINOR) \
                         -DRUBY_TEENY=$(RUBY_TEENY)
RUBY_CFLAGS            = @RUBY_CFLAGS@
RUBY_CXXFLAGS          = @RUBY_CXXFLAGS@
RUBY_LDFLAGS           = @RUBY_LDFLAGS@
RUBY_LIBS              = @RUBY_LIBS@
RUBY_LIBRUBYARG        = @RUBY_LIBRUBYARG@
RUBY_LIBRUBYARG_STATIC = @RUBY_LIBRUBYARG_STATIC@
RUBY_SITELIBDIR        = @RUBY_SITELIBDIR@

# flags to be passed to compilers
CFLAGS = @CFLAGS@
CXXFLAGS = @CXXFLAGS@
FFLAGS = @FFLAGS@
FFLAGS_NOSTATIC = $(FOPT) $(FOTH) $(FDBG)

# set up suffixes needed for various files
F77SUF = @F77SUF@
OBJSUF = @OBJSUF@
LIBSUF = @LIBSUF@

#
# Initialize other variables.
#

AR = @AR@
ARFLAGS = @ARFLAGS@

LDFLAGS = @LDFLAGS@
