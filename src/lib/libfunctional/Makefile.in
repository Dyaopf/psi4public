
srcdir = @srcdir@
VPATH = @srcdir@

include ../MakeVars

PSITARGET = $(top_objdir)/lib/libPSI_functional.a

TRUECXXSRC = $(notdir $(wildcard $(srcdir)/*.cc))
DEPENDINCLUDE = $(notdir $(wildcard $(srcdir)/*.h))

LIBOBJ = $(TRUECXXSRC:%.cc=%.o)

include ../MakeRules

install_inc:: $(DEPENDINCLUDE)
	$(MKDIRS) $(incdir)/libfunctional
	$(INSTALL_INCLUDE) $^ $(incdir)/libfunctional


ifneq ($(DODEPEND),no)
include $(LIBOBJ:%.o=%.d)
endif

wB97_functional.o:
	$(CXX) $(CXXDBG) -O0 $(PYTHON_INCLUDE) $(CXXDEF) $(CXXOTH) $(PSICPPFLAGS) -c $< $(OUTPUT_OPTION)

wB97X_functional.o:
	$(CXX) $(CXXDBG) -O0 $(PYTHON_INCLUDE) $(CXXDEF) $(CXXOTH) $(PSICPPFLAGS) -c $< $(OUTPUT_OPTION)
