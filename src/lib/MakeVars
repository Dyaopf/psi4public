
SHELL = /bin/sh

#
# Initialize variables that have been figured out by configure.
#

target = i386-apple-darwin11.0.0

prefix = /usr/local/psi4
exec_prefix = ${prefix}
bindir = ${exec_prefix}/bin
libdir = ${exec_prefix}/lib
incdir = ${prefix}/include
docdir = ${datarootdir}/doc/${PACKAGE_TARNAME}
manext = 5
mandir = ${datarootdir}/man
htmldir = ${docdir}
txtdir = $(docdir)/txt
datadir = ${datarootdir}
datarootdir = ${prefix}/share
pkgdatadir=$(datadir)/psi
top_srcdir = /Users/masaaki/psi4
# this file is used by the subdirectories, so top_objdir is relative to those
top_objdir = ../../..

PWD = $(shell pwd)
RANLIB = ranlib
YACC = @YACC@
CC = mpicc
CXX = mpicxx
FC = gfortran
MKDIRS = $(top_srcdir)/bin/mkdirs.sh
INSTALL = /usr/bin/install -c
INSTALL_INCLUDE = $(top_srcdir)/bin/installinc.sh
INSTALL_DATA = ${INSTALL} -m 644
INSTALL_PROGRAM = ${INSTALL}
MKDIRS = $(top_srcdir)/bin/mkdirs.sh

PSICPPFLAGS = -I. -I$(top_objdir)/include -I$(top_srcdir)/include -I$(top_srcdir)/src/lib -I$(top_objdir)/src/lib -I$(top_srcdir)/madness/include -I$(top_srcdir)/madness/src/lib -I$(top_objdir)/madness/include
CPPFLAGS = 

PYTHON_INCLUDE = -I/System/Library/Frameworks/Python.framework/Versions/2.7/include/python2.7 -I/System/Library/Frameworks/Python.framework/Versions/2.7/include/python2.7

CDEF = 
COPT = -O2
COTH = 
CDBG = 
CINC = $(CPPFLAGS) $(PSICPPFLAGS)

CXXDEF = 
CXXOPT = -O2
CXXOTH =  -fopenmp
CXXDBG = 
CXXINC = $(CPPFLAGS) $(PSICPPFLAGS)

FDEF = 
FOPT = -O
FOTH = 
FDBG = -g
FINC = $(CPPFLAGS) $(PSICPPFLAGS)
FSTATIC = 

# The suffix generated by the -M compiler option
CCDEPENDSUF = none
CXXDEPENDSUF = none
CCDEPENDFLAGS = -M
CXXDEPENDFLAGS = -M
CCDEPEND = $(CC)
CXXDEPEND = $(CXX)

# flags to be passed to compilers
CFLAGS = $(CDBG) $(COPT) $(CDEF) $(COTH) $(CINC)
CXXFLAGS = $(CXXDBG) $(CXXOPT) $(CXXDEF) $(CXXOTH) $(CXXINC)
FFLAGS = $(FDBG) $(FOPT) $(FOTH) $(FSTATIC)
FFLAGS_NOSTATIC = $(FOPT) $(FOTH) $(FDBG)

# set up suffixes needed for various files
F77SUF = f
OBJSUF = o
LIBSUF = a

#
# Initialize other variables.
#

AR = ar
ARFLAGS = r

LDFLAGS =  -fopenmp
