
srcdir = @srcdir@
VPATH = @srcdir@

include ../MakeVars

PSITARGET = $(top_objdir)/lib/libPSI_efp_solver.a

TRUECXXSRC = $(notdir $(wildcard $(srcdir)/*.cc))
DEPENDINCLUDE = $(notdir $(wildcard $(srcdir)/*.h)) ../../../libefp/libefp/src/efp.h
LIBOBJ = $(TRUECXXSRC:%.cc=%.o)

include ../MakeRules

install_inc:: $(DEPENDINCLUDE)
	$(MKDIRS) $(DESTDIR)$(incdir)/libefp_solver
	$(INSTALL_INCLUDE) $^ $(DESTDIR)$(incdir)/libefp_solver

ifneq ($(DODEPEND),no)
include $(LIBOBJ:%.o=%.d)
endif

