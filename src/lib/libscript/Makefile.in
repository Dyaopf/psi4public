
srcdir = @srcdir@
VPATH = @srcdir@

include ../MakeVars

PSITARGET = $(top_objdir)/lib/libPSI_script.a

TRUECXXSRC = script.cc python.cc

INC = script.h

LIBOBJ = $(TRUECXXSRC:%.cc=%.o)

include ../MakeRules

ifneq ($(DODEPEND),no)
include $(LIBOBJ:%.o=%.d)
endif

install_inc:: $(INC)
	$(MKDIRS) $(DESTDIR)$(incdir)/libutil
	$(INSTALL_INCLUDE) $^ $(DESTDIR)$(incdir)/libutil

