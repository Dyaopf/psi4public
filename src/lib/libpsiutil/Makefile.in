
srcdir = @srcdir@
VPATH = @srcdir@

include ../MakeVars

PSITARGET = $(top_objdir)/lib/libPSI_util.a

TRUECSRC = 

TRUECXXSRC = stl_string.cc combinations.cc exception.cc memory_manager.cc quad.cc

INC = libpsiutil.h ref.h class_macros.h memory_manager.h quad.h

LIBOBJ = $(TRUECSRC:%.c=%.o) $(TRUECXXSRC:%.cc=%.o)

include ../MakeRules

install_inc:: $(INC)
	$(MKDIRS) $(DESTDIR)$(incdir)/libpsiutil
	$(INSTALL_INCLUDE) $^ $(DESTDIR)$(incdir)/libpsiutil

ifneq "$(MAKECMDGOALS)" "clean"
include $(LIBOBJ:%.o=%.d)
endif

