srcdir = @srcdir@
VPATH = @srcdir@

include ../MakeVars

PSITARGET = $(top_objdir)/lib/libPSI_scf.a

CXXSRC = main.cc hfenergy.cc hf.cc rhf.cc rohf.cc

BINOBJ = $(CXXSRC:%.cc=%.o)

include ../MakeRules

ifneq ($(DODEPEND),no)
$(BINOBJ:%.o=%.d): $(DEPENDINCLUDE)
include $(BINOBJ:%.o=%.d)
endif

install_man:: scf.1
	$(MKDIRS) $(DESTDIR)$(mandir)/man1
	$(INSTALL_INCLUDE) $^ $(DESTDIR)$(mandir)/man1
