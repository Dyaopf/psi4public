srcdir = @srcdir@
VPATH = @srcdir@

include ../MakeVars
PSITARGET = $(top_objdir)/bin/$(shell basename `pwd`)
MODULE = false

LDLIBS += $(LAPACK) $(BLAS) $(PYTHON_LDFLAGS) -lpthread
CXXINC += -DINSTALLEDPSIDATADIR=\"$(pkgdatadir)\" $(PYTHON_INCLUDE) 

# NOTE: Keep PSI_parallel last in the PSI library list (but before boost). It contains the fprintf override.
PSILIBS = \
-lPSI_mints_wrapper -lPSI_dfmp2 -lPSI_scf -lPSI_ccenergy -lPSI_ccsort \
-lPSI_transqt2 -lPSI_cctriples -lPSI_mp2 -lPSI_lmp2 -lPSI_scf_solver -lPSI_mcscf -lPSI_psimrcc -lPSI_dcft \
-lPSI_sapt -lPSI_sapt_solver -lPSI_cchbar -lPSI_cclambda -lPSI_ccdensity -lPSI_oeprop -lPSI_transqt \
-lPSI_functional -lPSI_mints_deriv -lPSI_optking \
-lPSI_mints -lPSI_trans -lPSI_dpd -lPSI_chkpt -lPSI_iwl -lPSI_psio -lPSI_qt \
-lPSI_ciomr -lPSI_options -lPSI_util -lPSI_moinfo \
-lPSI_deriv -lPSI_int -lPSI_util -lPSI_diis -lPSI_plugin -lPSI_parallel -lboost_python -lboost_regex -lboost_thread \
-lboost_filesystem

CXXSRC = \
set_memory.cc \
psi4.cc psi4_driver.cc read_options.cc clean.cc \
setup_driver.cc \
psi_start.cc psi_stop.cc version.cc \
script.cc python.cc execute_bp.cc

BINOBJ = $(CXXSRC:%.cc=%.o)

include ../MakeRules

ifneq ($(DODEPEND),no)
$(BINOBJ:%.o=%.d): $(DEPENDINCLUDE)
include $(BINOBJ:%.o=%.d)
endif

