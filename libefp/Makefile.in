top_srcdir = @top_srcdir@
top_objdir = ..

CDEF = @CDEF@
COPT = @COPT@
COTH = @COTH@
CDBG = @CDBG@
CINC = @CINC@

CC = @CC@
CPPFLAGS = @CPPFLAGS@
CFLAGS = @CFLAGS@
LDFLAGS = @LDFLAGS@
AR = @AR@
ARFLAGS = @ARFLAGS@
OBJSUF = @OBJSUF@
MKDIRS = @MKDIRS@
RANLIB = @RANLIB@
srcdir = @srcdir@

VPATH = @srcdir@/libefp/src
PSITARGET = $(top_objdir)/libefp/libefp.a
TRUECSRC = $(notdir $(wildcard $(srcdir)/libefp/src/*.c))
LIBOBJ = $(TRUECSRC:%.c=%.o)

all: $(PSITARGET)

$(PSITARGET): $(LIBOBJ)
	$(MKDIRS) `dirname $(PSITARGET)`
	rm -f $(PSITARGET)
	$(AR) $(ARFLAGS) $(PSITARGET) $^
	$(RANLIB) $(PSITARGET)
