#! RHF-CCSD 6-31G** all-electron optimization of the H2O molecule

molecule h2o {
    O
    H 1 0.97
    H 1 0.97 2 103.0
}

set {
#    jobtype opt
    dertype first
    docc [3, 0, 1, 1]
    basis 6-31G**

    r_convergence 11
    e_convergence 11
    d_convergence 11
}

thisenergy = optimize('ccsd')



clean()
