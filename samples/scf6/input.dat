#! SCF DZ allene geometry optimzation, with Cartesian input


molecule allene {
  H -2.0  0.0  1.0
  H -2.0  0.0 -1.0
  C -1.5  0.0  0.0
  C  0.0  0.0  0.0
  C  1.5  0.0  0.0
  H  2.0  1.0  0.0
  H  2.0 -1.0  0.0
}

set global {
  basis DZ
  e_converge 10
  d_converge 10
}

# Hack variables
cycle = 0
returnValue = 0
thisenergy = 0.0

while (cycle < 20 and returnValue != PsiReturnType.EndLoop):
    thisenergy = energy('scf')
    deriv()
    returnValue = optking()
    cycle = cycle + 1


clean()
