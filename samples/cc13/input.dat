#! cc-pVDZ UCCSD optimization of triplet methtylene, with Z-matrix input


molecule ch2 {
  0 3
  C
  H 1 1.1
  H 1 1.1 2 109.0
}

set globals {
  reference uhf
  relax_opdm true
  basis cc-pvdz
  dertype first
  # The next 3 options control the geometry optimization tolerances
  conv_max_disp  1e-6
  conv_max_force 1.0e-6
  conv_max_de    7
  e_converge 10
  d_converge 10
}

optimize('CCSD')


clean()



