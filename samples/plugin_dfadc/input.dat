
molecule h2o {
    O
    H 1 0.9584
    H 1 0.9584 2 104.45
    symmetry c1
}

plugin_load("./plugin_dfadc.so")

set {
    reference rhf
    wavefunction ccd
    basis aug-cc-pvdz
    ri_basis_cc  aug-cc-pvdz
    ri_basis_adc aug-cc-pvdz
    guess core
    scf_type direct
    memory 2000.0
    num_roots 20
}

run_dfcc(h2o)
plugin("plugin_dfadc.so")

clean()
