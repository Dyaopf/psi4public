table = Table(rows=("Basis"), cols=("E(SCF)"))

globals {
  diis false
  ri_basis_scf cc-pvdz-jkfit
}

set scf {
  scf_type df
}

bases = [ "sto-3g", "cc-pvdz" ]

for basis in bases:
    global basis $basis

    PsiMod.IO.set_default_namespace(basis)

    escf = scf()

    table[basis] = escf

print str(table)

