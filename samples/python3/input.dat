molecule h2 {
H
H 1 .9

R = .9
}

Rs = [ 0.7, 1.0, 1.1, 3.0, 10.0 ]
escf = []

for val in Rs:
    h2.R = val

    set scf {
        SCF_TYPE DF
        PRINT 2
        DIIS False
        BASIS 3-21G
        RI_BASIS_SCF STO-3G
        DOCC [3, 0, 1, 1]
    }

    escf.append(scf())

    set dfmp2 {
        BASIS  3-21G
        RI_BASIS_MP2 STO-3G
    }

    edfmp2 = dfmp2()

    print "E(DFMP2) = %f" % (edfmp2)

