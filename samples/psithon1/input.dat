#! Spectroscopic constants of H2, and the full ci cc-pVTZ level of theory

molecule h2{
    H
    H 1 R
}

energies = []
rvals = [0.65, 0.7, 0.75, 0.8, 0.85]
set {
    basis cc-pvtz
    d_convergence 12
    e_convergence 12
    r_convergence 12
}

for r in rvals:
    h2.R = r
    energies.append(energy('fci'))

phys_consts = diatomic_anharmonicity(rvals, energies)
