#! 6-31G** H2O Test CISD Energy Point


molecule h2o {
    O
    H 1 1.00
    H 1 1.00 2 103.1
}

# Perform a detci calc using helper function
set globals {
  basis 6-31G**
  hd_ave hd_kave
}

thisenergy = energy('cisd')


clean()

# Perform same detci calc setting all options explicitly
set globals {
  wfn detci
  basis 6-31G**
}

set detci {
  hd_ave hd_kave
  ex_lvl 2
}

thisenergy = energy('detci')


clean()

