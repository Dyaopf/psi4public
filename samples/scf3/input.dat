#! UHF 6-31G** energy of the $^3B_1$ state of CH$_2$, with Z-matrix input. The occupations
#! are specified explicitly.


molecule ch2 {
    0 3
    c
    h 1 b1
    h 1 b1 2 a1

    b1 = 1.0
    a1 = 125.0
}

globals {
    reference uhf
    basis     6-31G**
    docc      [2, 0, 0, 1]
    socc      [1, 0, 1, 0]
}

mints()

thisenergy = energy('uhf')


clean()
